{
    "title": "PC",
    "rules": [
        {
            "description": "Home to Command Left",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "home"
                    },
                    "to": [
                        {
                            "key_code": "left_arrow",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "End to Command Right",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "end"
                    },
                    "to": [
                        {
                            "key_code": "right_arrow",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "F5 to Command R",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "f5"
                    },
                    "to": [
                        {
                            "key_code": "r",
                            "modifiers": [
                                "command"
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "description": "Command+L to Lock Screen",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "l",
                        "modifiers": {
                            "mandatory": [
                                "command"
                            ]
                        }
                    },
                    "to": [
                        {
                            "key_code": "q",
                            "modifiers": [
                                "left_control",
                                "left_command"
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
